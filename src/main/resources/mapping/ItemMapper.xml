<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.lizp.sec.cache.dao.ItemDao" >
    <select id="getById" resultType="com.lizp.sec.cache.entity.Item">
        select
        *
        from item
        where id = #{id}
    </select>

</mapper>